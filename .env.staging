# Staging Environment Configuration
# This file contains environment-specific settings for staging

# Environment identifier
VITE_ENV=staging

# AWS Amplify Configuration (Staging)
# These values should be replaced with actual staging environment values
VITE_AWS_REGION=us-east-1
VITE_USER_POOL_ID=us-east-1_YYYYYYYYY
VITE_USER_POOL_CLIENT_ID=YYYYYYYYYYYYYYYYYYYYYYYYYY
VITE_GRAPHQL_ENDPOINT=https://YYYYYYYYYYYYYYYYYYYYYYYYYY.appsync-api.us-east-1.amazonaws.com/graphql
VITE_S3_BUCKET=insightsphere-staging-exports

# API Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Monitoring
VITE_SENTRY_DSN=
VITE_SENTRY_ENVIRONMENT=staging
VITE_SENTRY_TRACES_SAMPLE_RATE=0.5

# Debug Settings
VITE_DEBUG_MODE=false
VITE_LOG_LEVEL=info

# Performance Settings
VITE_ENABLE_SOURCE_MAPS=true
VITE_BUNDLE_ANALYZER=false
